<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Camaras</title>
    </head>

    <body>
        <!-- libreria js de autocaptura SDK web -->
        <script src="https://sandbox-web-plugins.s3.amazonaws.com/autocapture/autocapture.js"></script>  
        
        <script src="https://sandbox-web-plugins.s3.amazonaws.com/liveness/js/liveness.js"></script>
         
        <script
  src="https://code.jquery.com/jquery-3.6.3.min.js"
  integrity="sha256-pvPw+upLPUjgMXY0G+8O0xUf+/Im1MZjXxxgOcBQBXU="
  crossorigin="anonymous"></script>


         <!-- libreria js de livenes SDK web -->
        <div class="container" id="container"></div>        
        <script>            
        $Variable ="";
            //llamar a la camara front
            TOCautocapture('container', {
                locale: "es",
                session_id: "<%= front %>",
                document_type: "CHL2",
                document_side: "front", //cara del documento
                http: "true",
                callback: function(captured_tokenF, image){ 
                    // alert(captured_tokenF);
                    container.innerHTML = "";
                    $Variable = captured_tokenF;
                    // llamar a la camara back
                    TOCautocapture('container', {
                        locale: "es",
                        session_id: "<%= back %>",
                        document_type: "CHL2",
                        document_side: "back", //cara del documento
                        http: "true",
                        callback: function(captured_tokenB, image){ 
                            // alert(captured_tokenB);
                            container.innerHTML = "";
                            // llamar a la camara selfie
                            TOCliveness('container', {  
                                locale: "es",  
                                session_id: "<%= selfie %>",  
                                http: "true",
                                callback: function(captured_tokenS){ alert("token Documento front: "+ captured_tokenF +", token documento back: " + captured_tokenB + ",token selfie: " + captured_tokenS); },  
                                failure: function(error){ alert(error); }  
                            });
                            TOCliveness();
                        },
                        failure: function(error){ alert(error); }, 
                    });
                    TOCautocapture();
                },
                failure: function(error){ alert(error); }, 
            });
            try{
                TOCautocapture();
            }
            catch(e){
                console.log("Hola ocurrio un error"+e)
            }
            console.log("soy variable"+$Variable);
        </script>
        
    </body>
</html>